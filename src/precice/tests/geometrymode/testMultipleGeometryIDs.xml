<?xml version="1.0"?>

<precice-configuration>
  
   <log-filter target="debug" component="precice" switch="off" />
  
   <solver-interface geometry-mode="yes" dimensions="2">     
      <mesh name="Quad0">
         <sub-id side="1"/>
         <sub-id side="2"/>
         <sub-id side="3"/>
      </mesh>
     
      <mesh name="Quad1">
         <sub-id side="1"/>
         <sub-id side="2"/>
         <sub-id side="3"/>
      </mesh>
      
      <geometry:builtin-cuboid of-mesh="Quad0">
         <discretization-width value="1.0"/> 
         <length value="5.0; 5.0"/>
      </geometry:builtin-cuboid>
     
      <geometry:builtin-cuboid of-mesh="Quad1">
         <offset value="4.0; 4.0"/>
         <discretization-width value="1.0"/>
         <length value="5.0; 5.0"/>
      </geometry:builtin-cuboid>
      
      <participant name="TestAccessor">
         <use-mesh name="Quad0"/>
         <use-mesh name="Quad1"/>
      </participant>
   </solver-interface>
</precice-configuration>